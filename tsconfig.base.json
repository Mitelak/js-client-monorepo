{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": false,
    "lib": [
      "es6",
      "dom",
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "paths": {
      "@sigstat/client-extensions": [
        "packages/client-extensions/src/index.ts"
      ],
      "@sigstat/core": [
        "packages/core/src/index.ts"
      ],
      "@sigstat/expo-bindings": [
        "packages/expo-bindings/src/index.ts"
      ],
      "@sigstat/on-device-evaluations": [
        "packages/on-device-evaluations/src/index.ts"
      ],
      "@sigstat/precomputed-evaluations": [
        "packages/precomputed-evaluations/src/index.ts"
      ],
      "@sigstat/react-bindings": [
        "packages/react-bindings/src/index.ts"
      ],
      "@sigstat/react-native-bindings": [
        "packages/react-native-bindings/src/index.ts"
      ],
      "@sigstat/sha256": [
        "packages/sha256/src/index.ts"
      ],
      "statsig-test-helpers": [
        "packages/test-helpers/src/index.ts"
      ]
    },
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "tmp",
    "tools",
  ]
}