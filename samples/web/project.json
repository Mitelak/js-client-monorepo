{
  "name": "web-sample",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "samples/web/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/samples/web",
        "compiler": "babel",
        "main": "samples/web/src/main.ts",
        "tsConfig": "samples/web/tsconfig.app.json",
        "webpackConfig": "samples/web/webpack.config.js",
        "assets": [],
        "index": "samples/web/src/index.html",
        "baseHref": "/",
        "styles": [],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": []
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "web-sample:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "web-sample:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["samples/web/**/*.ts"]
      }
    }
  }
}
