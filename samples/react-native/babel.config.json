{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "version": "2023-05"
      }
    ],
    "@babel/plugin-transform-class-static-block",
    [
      "@babel/plugin-transform-private-methods",
      {
        "loose": true
      }
    ]
  ]
}