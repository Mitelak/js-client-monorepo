{
  "name": "statsig-js-monorepo",
  "version": "1.0.0",
  "description": "A monorepo in TypeScript",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build --verbose && webpack && npm run size-check",
    "size-check": "./size-check.js",
    "test": "jest",
    "clean-build": "npm run clean && npm run build",
    "clean": "npm run clean-build-folders && npm run clean-tsbuild-info",
    "clean-build-folders": "find . -type d \\( -name 'build' -o -name 'dist' \\) -not -path './node_modules/*' -exec rm -rf {} +",
    "clean-tsbuild-info": "find . -type f -name 'tsconfig.tsbuildinfo' -not -path './node_modules/*' -exec rm -f {} +",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/webpack": "^5.28.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "terser": "^5.19.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@statsig/core": "file:./packages/core",
    "@statsig/local-eval": "file:./packages/local-eval",
    "@statsig/react": "file:./packages/react",
    "@statsig/remote-eval": "file:./packages/remote-eval",
    "@statsig/extensions": "file:./packages/extensions",
    "typescript": "^4.9.3"
  }
}