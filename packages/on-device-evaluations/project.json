{
  "name": "on-device-evaluations",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/on-device-evaluations/src",
  "projectType": "library",
  "targets": {
    "build": {
      "command": ":",
      "dependsOn": ["^build", "build-webpack"]
    },
    "build-ts": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/on-device-evaluations",
        "main": "packages/on-device-evaluations/src/index.ts",
        "tsConfig": "packages/on-device-evaluations/tsconfig.lib.json",
        "assets": ["packages/on-device-evaluations/*.md"]
      }
    },
    "publish": {
      "command": "ts-node ./tools/scripts/publish.ts on-device-evaluations",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/on-device-evaluations/**/*.ts",
          "packages/on-device-evaluations/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/on-device-evaluations/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build-webpack": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "statsJson": true,
        "verbose": true,
        "outputPath": "overridden in webpack config",
        "main": "overridden in webpack config",
        "tsConfig": "packages/on-device-evaluations/tsconfig.lib.json",
        "webpackConfig": "packages/on-device-evaluations/webpack.config.js"
      },
      "dependsOn": ["build-ts"]
    }
  },
  "tags": []
}
