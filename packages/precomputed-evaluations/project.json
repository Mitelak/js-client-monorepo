{
  "name": "precomputed-evaluations",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/precomputed-evaluations/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/precomputed-evaluations",
        "main": "packages/precomputed-evaluations/src/index.ts",
        "tsConfig": "packages/precomputed-evaluations/tsconfig.lib.json",
        "assets": ["packages/precomputed-evaluations/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs precomputed-evaluations {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/precomputed-evaluations/**/*.ts",
          "packages/precomputed-evaluations/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/precomputed-evaluations/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
